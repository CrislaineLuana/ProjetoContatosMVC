@model List<ContatoModel>


 <table id="table-contatos-usuario" class="table table-striped table-hover">
                      <thead>
                        <tr>
                          <th scope="col">#</th>
                          <th scope="col">Nome</th>
                          <th scope="col">E-mail</th>
                          <th scope="col">Celular</th>

                        </tr>
                      </thead>
                      <tbody>
                    @if(Model != null && Model.Any())
                    {
                        @foreach(var item in Model)
                                    {
                                        <tr>
                                          <th scope="row">@item.Id</th>
                                          <td>@item.Nome</td>
                                          <td>@item.Email</td>
                                          <td>@item.Celular</td>
                                        </tr>

                                     }
                     }   
                     else
                    {
                       <tr>
                           <td valign="top" colspan="4" >Nenhum contato encontrado</td>
                       </tr>
                    }
    </tbody>
 </table>
